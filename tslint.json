{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"variable-name": false,
		"prefer-const": false,
		"no-console": false,
		"one-line": false,
		"prefer-for-of": false,
		"max-line-length": [
			true,
			{
				"length": 200
			}
		],
		"array-element-newline": false,
		"brace-style": [
			true,
			"allman",
			{
				"allowSingleLine": true
			}
		]
	},
	"rulesDirectory": []
}